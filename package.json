{
  "name": "Xchat",
  "version": "1.0.0",
  "description": "A chat application",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "egg": "^2.15.1",
